RewriteEngine On
# Deze regel is cruciaal wanneer je een Alias of submap /admin gebruikt
RewriteBase /admin

# Als een bestand of map echt bestaat, serveer dat normaal
RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^ - [L]

# Anders alles naar index.php
RewriteRule ^ index.php [L]
